{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false
  },
  "module": {
    "type": "es6"
  },
  "minify": true,
  "sourceMaps": true,
  "env": {
    "targets": {
      "chrome": "88",
      "firefox": "85",
      "safari": "14",
      "edge": "88"
    }
  }
}
